  padding: 10px 20px;
  font-weight: 600;
  font-size: 12px;
}

.article-title{
  margin: 50px 0 0 0;
}

.article-contents-two{
  padding: 50px 0;
  background-image: url('banner-bg.png');
}

#comments .number_of_comments{
  padding: 0;
  margin: 0;
}

#comments .number_of_comments > li{
  list-style: none;
  border-bottom: 1px solid #ccc;
}

#comments .form-success{
  margin-bottom: 20px;
}

.commentform{
  padding: 50px 0;
}

.number_of_comments li{
  position: relative;
}

.number_of_comments li:before{
  content: "";
      position: absolute;
      top: -20px;
      left: -45px;
      z-index: 999;
      background-image: url('banner-bg.png');
      background-repeat: no-repeat;
      height: 70px;
      width: 70px;
}

.number_of_comments li:after{
  content: "";
  position: absolute;
  bottom: 20px;
  right: -25px;
  z-index: 999;
  background-image: url('banner-bg.png');
  background-repeat: no-repeat;
  height: 70px;
  width: 70px;
  -webkit-transform: scaleX(-1);
  transform: scaleX(-1);
}

.my-account{
  vertical-align: middle;
}

.account-icon{
  color: #fff;
  height: 17px;
  margin-top: 4px;
}

.check-inline > *{
  display: inline-block;
}

.check-inline input[type=checkbox]{
  width: 20px;
}

.top-padding{
  margin-top: 50px;
}

.mobile-login{
  background-color: #ccc;
}

.mobile-logout{
  color: red;
}

.forgot_password{
  float: right;
}

.errors ul, .errors ul li{
  padding: 0;
  margin: 0 0 10px 0;
  list-style: none;
  color: red;
}

.form-success{
  margin-bottom: 10px;
}

.guest_continue{
  margin-top: 10px;
  text-align: center;
}

.payment-icons, .payment-icons li{
  padding: 0;
  margin: 0;
  list-style: none;
}

.footer-payment_methods{
  text-align: right;
}

.footer-payment_methods > *, .payment-icons li{
  display: inline-block;
}

.payment-icons li svg{
  height: 30px;
  width: 50px;
}

.breadcrumbs-section{
  padding: 10px 0;
}

@media(max-width: 1200px){
  .slider-info h2{
    font-size: 34px;
  }

  .slider-info p{
    margin-bottom: 20px;
  }
}


@media(max-width: 991px){
  .top-banner .social-media,
  .header-section .menu-right,
  .main-menu nav{
    display: none;
  }

  header.header-section{
    position: relative;
    background-color: #000;
  }

  header .main-logo * a, header .main-logo h1 a{
    color: #fff;
  }

  .mobile-menu-icon .cart-head, .mobile-menu-icon .search-button{
    float: right;
    width: 25px;
    height: 25px;
    padding: 0;
    margin: -2px 10px;
  }

  .item_count{
    right: -7px;
  }

  .mobile-menu-icon .cart-head svg{
    height: 25px;
    width: 25px;
  }

  .mobile-menu-icon .search-button svg{
    height: 25px;
    width: 35px;
  }

  header .main-logo h1{
    font-size: 34px;
  }

  .template-index header.header-section {
    padding: 20px 0;
  }

  header .main-menu{
    padding-top: 5px;
  }

  .home-slider:after{
    display: none;
  }

  .flex-control-nav{
    padding: 0;
    text-align: center;
  }

  .flexslider .slides li{
    height: 400px;
  }

  .flexslider .slides img{
    width: auto;
    margin: auto;
    max-width: none;
    height: 100%;
  }

  .about-info{
    padding: 120px 20px 50px;
  }

  .about-info h2:before{
    left: 0;
    width: 100%;
  }

  .banner-section .banner-info h1,
  .newsletter-section h1{
    font-size: 34px;
  }

  .newsletter-section .newsletter-subheading{
    font-size: 18px;
  }
  .insta-slider #insta-feed > a{
    display: none;
  }
  #insta-feed > a:first-child,
  #insta-feed > a:nth-child(2),
  #insta-feed > a:nth-child(3),
  #insta-feed > a:nth-child(4),
  #insta-feed > a:nth-child(5),
  #insta-feed > a:nth-child(6),
  #insta-feed > a:nth-child(7),
  #insta-feed > a:nth-child(8){
    display: inline-block;
    width: 25% !important;
  }
  #insta-feed .instafeed-container,
  .instafeed-shopify .instafeed-container{
    width: 100% !important;
    padding-top: 100% !important;
  }
  .product-atc-btn, .qtn-number{
    padding: 6px 10px;
  }
  .qtn-number{
    font-size: 12px;
    width: 100px;
  }
  .minus-btn, .plus-btn{
    height: 30px;
    width: 30px;
    font-size: 16px;
  }
  .cart-bottom{
    padding-bottom: 20px;
  }
  .collection-banner-title .collection-title{
    font-size: 32px;
  }
  .main-contact-info{
    margin-top: 20px;
  }

  .contact-form-holder{
    margin-top: 20px;
  }

  .insta-slider #insta-feed > a.instafeed-lightbox{
    width: 100% !important;
  }
  .forgot_password{
    float: none;
  }
  .login-links{
    text-align: center;
  }
  .copyright-section .copyright-info.col-md-6, .footer-payment_methods{
    padding: 5px 0;
    text-align: center !important;
  }
  .footer-payment_methods > span{
    display: none;
  }
  .about-section-two .col-md-6{
    width: 100%;
    padding: 20px 0 !important;
  }
}


@media(max-width: 767px){
  .full-collection-holder .featured-collection-item{
    max-width: 450px;
    margin: auto;
  }
  .full-collection-holder{
    margin-bottom: 40px;
  }
  .footer-payment_methods{
    text-align: center;
  }
  .flex-align{
    display: block;
    align-items: normal;
  }
  .banner-info{
    text-align: center !important;
    margin-bottom: 20px;
  }
  .newsletter-info{
    text-align: center !important;
  }
  #contact_form{
    padding: 0 20px;
  }
  #contact_form > *{
    width: 100%;
  }
  .newsletter-section #Email{
    width: 100%;
    margin-bottom: 10px;
  }
  .insta-slider #insta-feed > a{
    display: none;
  }
  .insta-slider #insta-feed > a:first-child,
  .insta-slider #insta-feed > a:nth-child(2),
  .insta-slider #insta-feed > a:nth-child(3),
  .insta-slider #insta-feed > a:nth-child(4){
    display: inline-block;
    width: 50% !important;
  }
  #insta-feed .instafeed-container,
  .instafeed-shopify .instafeed-container{
    width: 100% !important;
    padding-top: 100% !important;
  }
  .newsletter-section #Email{
    text-align: center;
  }
  .brands-section .brands-imgs a{
    min-height: 0;
  }
  .brands-section .brands-imgs a img{
    position: relative;
  }
  .minus-btn, .plus-btn{
    display: none;
  }
  .cart-product-quantity{
    text-align: center;
  }
  .cart-qty{
    text-align: center;
  }
  .cart-separator > div{
    display: block !important;
  }
  .responsive-table .cart-product-info{
    font-size: 12px;
  }
  .cart-price{
    font-size: 16px;
  }

  .footer-links .col-xs-12 .footer-link ul li{
    width: 50%;
    float: left;
  }

  .mission-statement{
    margin-top: 20px;
  }

  .footer-links{
    text-align: center;
  }
  .fourofour-holder{
    min-height: 300px;
  }
  .fourofour h1{
    font-size: 34px;
  }
  .about-section-one{
    text-align: center;
    padding: 0 0 50px 0;
  }

  .insta-slider #insta-feed > a.instafeed-lightbox{
    width: 100% !important;
  }
  .article-content{
    padding: 50px 20px;
  }
  #comments .number_of_comments > li{
    padding: 30px 20px;
  }
  .number_of_comments li:before {
    top: -10px;
    left: -15px;
  }
  .number_of_comments li:after {
    bottom: 20px;
    right: 25px;
  }
  .new_address{
    text-align: center;
  }
  .address-form [data-line-count="2"],
  .address-form [data-line-count="3"]{
    flex-basis: 100%;
  }
  .home-slider .section-arrow{
    display: none;
  }
}



@media(max-width: 680px){
  .cart-separator > div{
    display: block !important;
    width: 100%;
  }

  .cart-header{
    display: none;
  }

  .cart-separator .cart-info-section,
  .cart-separator .cart-product-info,
  .cart-separator .cart-product-quantity,
  .cart-separator .cart-price{
    display: block !important;
    width: 100%;
    padding: 0;
    margin: 5px 0;
    font-size: 16px;
  }

  .cart-separator{
    text-align: center;
    border: 1px solid #eee !important;
    margin-bottom: 30px;
  }
  .checkout-btns > *{
    width: 100%;
    display: block;
    font-size: 12px;
    text-align: center;
    padding: 6px 0;
    margin-bottom: 10px;
  }
  .about-year{
    font-size: 110px;
  }
  .about-section-three .title.mass-padding {
    padding: 50px 0;
  }

  .about-section-two, .about-section-three{
    padding: 50px 0;
  }

  .insta-slider #insta-feed > a.instafeed-lightbox{
    width: 100% !important;
  }

}


.menu-open {
    overflow: hidden;
}
.mobile-menu {
    position: fixed;
    top: 0;
    // right: 0;
    bottom: 0;
    left: 0;
    z-index: 999999;
    font-weight: 600;
    display: none;
    overflow: hidden;
    -webkit-overflow-scrolling: touch;
    outline: 0;
    background-color: rgba(0, 0, 0, 0.5);
}
.menu-open .mobile-menu {
    overflow-x: hidden;
    overflow-y: auto;
}
.mobile-menu-inner {
    background-color: #fff;
    padding: 30px 20px;
    width: 250px;
    margin-left: 0;
    margin-right: auto;
    min-height: 100vh;
    position: relative;
    -webkit-transition: -webkit-transform .3s ease-out;
    -o-transition: -o-transform .3s ease-out;
    transition: transform .3s ease-out;
    -webkit-transform: translate(-250px, 0);
    -ms-transform: translate(-250px, 0);
    -o-transform: translate(-250px, 0);
    transform: translate(-250px, 0);
}
.menu-open .mobile-menu-inner {
    -webkit-transform: translate(0, 0);
    -ms-transform: translate(0, 0);
    -o-transform: translate(0, 0);
    transform: translate(0, 0);
}
.mobile-menu-close a {
    color: #000;
    display: inline-block;
    line-height: 1;
    font-size: 24px;
    text-decoration: none;
}
.mobile-menu-close a:after {
    content: '\e805';
    font-family: "fontello";
}
.mobile-menu-widget {
    margin: 20px 0;
}
.mobile-menu-widget h3 {
    margin: 0;
    font-size: 16px;
    text-transform: uppercase;
    font-weight: bold;
    padding-bottom: 10px;
    margin-bottom: 10px;
    line-height: 1;
    border-bottom: 1px solid #ececec;
}
.mobile-menu-widget ul {
    padding: 0;
    margin: 0;
    list-style: none;
}
.mobile-menu-widget ul li {
    margin: 0 0 0;
}
.mobile-menu-widget ul li a {
    padding: 0;
    line-height: 40px;
    font-size: 13px;
    background-color: transparent !important;
    display: block;
    color: #555;
    text-decoration: none !important;
}
.mobile-menu-widget ul li a:hover {
    color: #333;
}
.mobile-menu-widget ul ul {
    display: none;
}
.mobile-menu-widget ul li a:after {
    display: none;
}

.mobile-menu-close {
    text-align: right;
    padding: 0 20px 10px 0;
    border-bottom: 1px solid #eee;
}
.mobile-menu-inner {
    padding: 10px 0;
}
.mobile-menu-widget h3 {
    padding: 10px;
    margin-bottom: 0;
}
.mobile-menu-widget ul li {
    border-bottom: 1px solid #eee;
}
.mobile-menu-widget ul li a {
    padding: 0 20px;
    text-transform: uppercase;
}
#mobile-categories ul li.dropdown {
    position: relative;
}
.mobile-menu-inner li > .dropdown-menu a {
    padding-left: 40px;
    font-weight: 600;
}
.mobile-menu-inner li .dropdown-menu{
  display: block;
  position: relative;
  width: 100%;
  box-shadow: none;
  border: none;
  border-top: 1px solid #eee;
}
.mobile-menu-widget ul li.dropdown .subcat-dropdown {
    position: absolute;
    display: block;
    top: 0;
    right: 0;
    height: 100%;
    width: 50px;
    padding: 9px 20px;
    border-left: 1px solid #eee;
    background-color: transparent;
    z-index: 99999;
}
// .mobile-menu-widget ul li.dropdown .subcat-dropdown:before {
//     content: "\f107";
//     font-family: "fontello";
// }
.mobile-menu-widget ul li.dropdown:hover .subcat-dropdown {
    cursor: pointer;
}
